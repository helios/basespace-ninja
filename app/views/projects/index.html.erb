<table class="table table-striped table-condensed table-hover" >
<tr>
<% @results[0][0].swagger_types.keys.each do |key| %>

  <th><%= key %>
  </th>
<% end %>
</tr>
<% @results[0].each do |row|  %>
	<tr>
	 <% row.attributes.values.each do |value| %>
	 <td>
	 <%= value %>
	 </td>
	 <%end %>
	 </tr>
<% end %>
</table>